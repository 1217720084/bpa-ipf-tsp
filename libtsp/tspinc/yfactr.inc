*BEGIN YFACTR
      DOUBLE PRECISION GTRI, BTRI, RDIAR, RDIAX
      COMMON /YFACTR/ GTRI(5*MAXBUS), BTRI(5*MAXBUS), RDIAR(MAXBUS),
     1     RDIAX(MAXBUS), INDXZ(3*MAXBUS), INDEX(MAXBUS+4),
     2     ISTORT(150)
      DOUBLE PRECISION AMTRX
      DIMENSION AMTRX(17*MAXBUS)
      EQUIVALENCE (AMTRX(1),GTRI(1))
      DIMENSION ILOC(3*MAXBUS)
      COMMON/JSPOTT/JSPOT(MAXBUS)
      DIMENSION ILOCF(MAXBUS)
      EQUIVALENCE (ILOCF, INDEX), (INDXZ, ILOC)
