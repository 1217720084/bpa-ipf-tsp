   i_pen 2 eq{             % draw command                                                 
     pen_open 1 eq{                                                                       
       x_pen y_pen lineto                                                                 
       stroke} if                                                                         
     newpath                                                                              
     /pen_open 1 def                                                                      
     x_pen y_pen moveto                                                                   
